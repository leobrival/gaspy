<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>


<% @products.each do |product| %>
  <div>
    <%= product.name %>
    <%= product.description %>
    <%= product.pricing %>
    <%= product.quantity %>
    <%= product.category %>
    <%= link_to "afficher produit", product_path(product) %>
  </div>
<% end %>

